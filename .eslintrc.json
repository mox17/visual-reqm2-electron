{
    "env": {
        "browser": true,
        "node": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "ignorePatterns": ["vql-parser.js"],
    "rules": {
        "no-unused-vars": [2, {"args": "after-used", "argsIgnorePattern": "^_"}]
    },
    "globals": {
        "process" : true,
        "Split": true,
        "Diff": true,
        "svgPanZoom": true,
        "settingsPopup": true,
        "ClipboardItem": true
    }
}
