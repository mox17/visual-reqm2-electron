  <inputfiles>
    <inputfile>for-oreqm-rules.reqm</inputfile>
  </inputfiles>
  <configuration>
    <doctypes>
      <regexs>
        <regex>^demospec$</regex>
        <regex>^tcs$</regex>
        <regex>^req$</regex>
        <regex>^feat$</regex>
      </regexs>
    </doctypes>
    <rules>
      <rule>
        <name>no_tcs_to_feat</name>
        <conditionchecks>
          <conditioncheck>
            <applyto>//specobject[(../@doctype='feat') and (./needscoverage/needsobj='tcs')]</applyto>
            <condition>false()</condition>
          </conditioncheck>
        </conditionchecks>
      </rule>
      <rule>
        <name>empty-rule</name>
      </rule>
      <rule>
        <name>draft_no_req_cov</name>
        <description>Just an example: a 'draft' specobject shall not require coverage</description>
        <conditionchecks>
          <conditioncheck>
            <applyto>//specobject[(./status='draft')]</applyto>
            <condition>count(./needscoverage)=0</condition>
          </conditioncheck>
          <conditioncheck>
            <applyto>//specobject[(./status='draft')]</applyto>
            <condition>count(./fulfilledby)=0</condition>
          </conditioncheck>
        </conditionchecks>
      </rule>
      <rule>
        <name>my_global_rule</name>
        <description>Just an example: No doctype other than 'feat'</description>
        <conditionchecks>
          <conditioncheck>
            <applyto>//specobjects[not(@doctype='feat')]</applyto>
            <condition>false()</condition>
          </conditioncheck>
        </conditionchecks>
      </rule>
      <rule>
        <name>my_regex_rule</name>
        <regexchecks>
          <regexcheck>
            <applyto>//specobject[(../@doctype='tcs') and (./status='rejected')]/comment</applyto>
            <regex>For regex condition rule</regex>
          </regexcheck>
        </regexchecks>
      </rule>
      <rule>
        <name>my_mixed_rule</name>
        <description>conditionchecks + regexchecks</description>
        <conditionchecks>
          <conditioncheck>
            <applyto>//specobjects[(./version='1.2') and (status='approved')]</applyto>
            <condition>false()</condition>
          </conditioncheck>
        </conditionchecks>
        <regexchecks>
          <regexcheck>
            <applyto>//specobject[(../@doctype='fea') and (./status='draft')]/description</applyto>
            <regex>fo+</regex>
          </regexcheck>
        </regexchecks>
      </rule>
    </rules>
    <oreqmRules>
      <rule>
        <name>no-more-than-one-incoming-ffb</name>
        <conditionchecks>
          <conditioncheck>
            <applyto>//specobject[count(./incomingFfbLinks/incomingFfbLink) &gt; 1]</applyto>
            <condition>false()</condition>
          </conditioncheck>
        </conditionchecks>
      </rule>
      <rule>
        <name>no-more-than-one-incoming-provcov</name>
        <conditionchecks>
          <conditioncheck>
            <applyto>//specobject[count(./needscoverage/needscov/coveredby/linkedfrom) &gt; 1]</applyto>
            <condition>false()</condition>
          </conditioncheck>
        </conditionchecks>
      </rule>
      <rule>
        <name>global-regex-check</name>
        <description>Results in a (global) violation if toolinfo doesn't contain 'foo'.</description>
        <regexchecks>
          <regexcheck>
            <applyto>/tracingresults/environment/toolversion</applyto>
            <regex>foo</regex>
          </regexcheck>
        </regexchecks>
      </rule>
    </oreqmRules>
  </configuration>
